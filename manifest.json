{
    "manifest_version": 2,
    "name": "Dark Mode",
    "version": "1.0",
    "description": "Force Dark mode for PDF files",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  